 "client_script": "frappe.web_form.on('resume_link', (field, value) => {\n    if (!frappe.utils.is_url(value)) {\n        frappe.msgprint(__('Resume link not valid'));\n    }\n});\n\n\n\nfrappe.web_form.validate = () => {\n    console.log(\"Before save event triggered\");\n\n    // Get the values of job_title and email_id fields\n    var jobTitle = frappe.web_form.get_value('job_title');\n    var email = frappe.web_form.get_value('email_id');\n\n    console.log(\"Job Title:\", jobTitle);\n    console.log(\"Email:\", email);\n\n    // Check if the job title and email are valid\n    if (jobTitle && email) {\n        console.log(\"Validation started\");\n\n        // Check if the email is already used for this job title\n        frappe.call({\n            method: \"custom_app.custom_script.validate_job_application\",\n            args: {\n                job_title: jobTitle,\n                email: email\n            },\n            callback: function(response) {\n                console.log(\"Validation response:\", response);\n\n                if ( response.message  === \"False\") {\n                    console.log(\"Validation error:\", response.message.message);\n                    frappe.msgprint('An application with this job title and email already exists.');\n                    // Manipulate the form elements to indicate the error\n                    $('[data-fieldname=\"email_id\"]').addClass('is-invalid'); // Example, you may need to adjust this selector based on your HTML structure\n                    $('[data-fieldname=\"email_id\"]').siblings('.invalid-feedback').html(response.message.message); // Example, you may need to adjust this selector based on your HTML structure\n                    // Prevent form submission\n                    return false;\n                } else {\n                    console.log(\"Validation successful\");\n                    // Allow form submission if validation is successful\n                    return true;\n                }\n            }\n        });\n    } else {\n        console.log(\"Invalid job title or email\");\n        // Prevent form submission if fields are not valid\n        return false;\n    }\n};\n\n\n",
